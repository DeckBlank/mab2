<footer class="c-footer padding-vertical-2 bg-dark">
  <div class="grid-container">
    <div class="c-footer__content flex-container">
      <div class="c-contacts flex-container">
        <div class="c-contact flex-container align-middle white">
          <p class="margin-bottom-0 fs-18">{{information.email}}</p>
        </div>
        <div class="c-contact flex-container align-middle white">
          <p class="margin-bottom-0 fs-18">{{information.phone}}</p>
        </div>
      </div>      
      <div class="c-vectors flex-container">
        {% for item in [1,2,3,4,5,6] %}          
          <figure class="c-vectors__item">
            <img class="of--content" src="{{theme.link}}/static/images/landing/footer_img/footer_{{item}}.png?key=20200406" alt="">
          </figure>
        {% endfor %}
      </div>
    </div>
  </div>
</footer>
